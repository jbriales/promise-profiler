{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc"
		]
	},
	"source": {
		"include": [
			"./src"
		],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_",
		"exclude": [
			"node_modules",
			"docs"
		]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"destination": "./docs/",
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "./node_modules/minami",
		"package": "./package.json",
		"readme": "./README.md"
	}
}