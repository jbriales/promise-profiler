{
  "name": "bluebird-promise-profiler",
  "version": "1.1.8",
  "description": "This repo will profile your code by giving the execution time of all promises in a json without disturbing the production level code.",
  "main": "./src/promise_profiler.js",
  "scripts": {
    "test": "BLUEBIRD_DEBUG=1 NODE_ENV=test ./node_modules/.bin/mocha test --recursive -t 10000",
    "test-with-coverage": "./node_modules/.bin/nyc --check-coverage --functions 100 --lines 95 --per-file --reporter=text npm test",
    "examples": "cd ./examples && npm i && npm test && cd ..",
    "generate-docs": "./node_modules/.bin/jsdoc --configure .jsdoc.json --verbose --readme README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajulbabel/promise-profiler.git"
  },
  "keywords": [
    "nodejs",
    "profiler",
    "promise",
    "bluebird",
    "javascript",
    "promise-profiler",
    "bluebird-promise"
  ],
  "author": "Rajul Babel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajulbabel/promise-profiler/issues"
  },
  "homepage": "https://github.com/rajulbabel/promise-profiler#readme",
  "dependencies": {
    "performance-now": "^2.1.0",
    "sinon": "^4.3.0"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "mocha": "^5.0.0",
    "mock-require": "^3.0.1",
    "nyc": "^11.4.1",
    "should": "^13.2.1"
  }
}
